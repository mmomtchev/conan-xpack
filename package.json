{
  "name": "@mmomtchev/conan-xpack",
  "version": "2.22.1-2",
  "description": "A xPack with conan",
  "scripts": {
    "postversion": "git push && git push --tags && node ./publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/conan-xpack.git"
  },
  "keywords": [
    "conan",
    "node-api",
    "swig",
    "xpack"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmomtchev/conan-xpack/issues"
  },
  "homepage": "https://github.com/mmomtchev/conan-xpack#readme",
  "xpack": {
    "minimumXpmRequired": "0.19.1",
    "binaries": {
      "destination": "./.content",
      "rootUrl": "https://github.com/mmomtchev/conan-xpack/releases/download",
      "skip": 1,
      "platforms": {
        "darwin-arm64": {
          "fileName": "conan-xpack-2.22.1-2-darwin-arm64.tar.gz",
          "sha256": "8d2932857931d8427ff708c56266393d3e08e8fee4af0254dae577065a02098f"
        },
        "darwin-x64": {
          "fileName": "conan-xpack-2.22.1-2-darwin-x64.tar.gz",
          "sha256": "a33ab1c233ef6bef973e3456b340a46f121058b782fd0ca955b47617124bb85c"
        },
        "linux-x64": {
          "fileName": "conan-xpack-2.22.1-2-linux-x64.tar.gz",
          "sha256": "c816c8bc69512727729a9af6dd79041dedbb85a2db2d9268e2a2bd675a5a74ea"
        },
        "win32-x64": {
          "fileName": "conan-xpack-2.22.1-2-win32-x64.zip",
          "sha256": "448142be412d7a66997629747a76d95d0891ba041bb7039e5c3e2e97bfcb38dd"
        },
        "linux-arm64": {
          "fileName": "conan-xpack-2.22.1-2-linux-arm64.tar.gz",
          "sha256": "da1e31fbb51292436c13c207e35cbe2c2b4f2923282550f242bca427d746d631"
        },
        "win32-arm64": {
          "fileName": "conan-xpack-2.22.1-2-win32-arm64.zip",
          "sha256": "9fad5590c69a272a678edabcf29a83bd9e68d0d5cf9df586785fdf0efbe7c47b"
        }
      },
      "baseUrl": "https://github.com/mmomtchev/conan-xpack/releases/download/v2.22.1-2"
    },
    "bin": {
      "conan": "./.content/bin/conan"
    },
    "properties": {
      "versionConan": "2.22.1",
      "commandInstall": "python -m pip install --target content conan=={{ properties.versionConan }}",
      "commandPostInstall": {
        "win32": "copy wrappers\\conan content\\bin\\conan && cl wrappers\\wrapper.c /Fe:content\\bin\\conan.exe /link",
        "linux": "cp wrappers/conan content/bin/conan",
        "darwin": "cp wrappers/conan content/bin/conan"
      }
    },
    "actions": {
      "build": [
        "{{ properties.commandInstall }}",
        "{{ properties.commandPostInstall[os.platform] }}"
      ],
      "test": [
        ".{{ path.sep }}content{{ path.sep }}bin{{ path.sep }}conan version",
        ".{{ path.sep }}content{{ path.sep }}bin{{ path.sep }}conan profile show"
      ]
    },
    "devDependencies": {
      "@mmomtchev/python-xpack": {
        "specifier": "3.12.11-2",
        "local": "link",
        "platforms": "all"
      }
    }
  },
  "dependencies": {
    "xpm": "^0.19.1"
  }
}